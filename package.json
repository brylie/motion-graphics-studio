    "@storybook/sveltekit": "^10.1.11",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.49.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^22",
    "@types/uuid": "^10.0.0",
